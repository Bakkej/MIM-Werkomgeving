        skos:prefLabel   "authentiek"@nl .

mimbegrip:Gegevensgroep
        a                skos:Concept ;
        rdfs:label       "Gegevensgroep"@nl ;
        dct:source       mimdoc:sectie2.2.3 ;
        skos:broader     mimbegrip:Modelelement ;
        skos:definition  "Een typering van een groep van gelijksoortige gegevens die voor een objecttype van toepassing is."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "Gegevensgroep"@nl .

mimbegrip:Codelijst  a   skos:Concept ;
        rdfs:label       "Codelijst"@nl ;
        dct:source       mimdoc:sectie2.4.5 ;
        skos:broader     mimbegrip:Datatype ;
        skos:definition  "De representatie van een lijst met een opsomming van de mogelijke domeinwaarden van een attribuutsoort, die buiten het model in een externe waardenlijst worden beheerd. De domeinwaarden in de lijst kunnen in de loop van de tijd aangepast, uitgebreid, of verwijderd worden, zonder dat het informatiemodel aangepast wordt (in tegenstelling tot bij een enumeratie). De representatie bevat geen kenmerken, voor alle kenmerken wordt verwezen naar de specificatie van de externe waardelijst."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "Codelijst"@nl .

mimbegrip:formeelPatroon
        a                skos:Concept ;
        rdfs:label       "formeel patroon"@nl ;
        dct:source       mimdoc:sectie2.8.2.26 ;
        skos:definition  "Zoals patroon, formeel vastgelegd, uitgedrukt in een formele taal die door de computer wordt herkend."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "formeel patroon"@nl .

mimbegrip:locatie  a     skos:Concept ;
        rdfs:label       "locatie"@nl ;
        dct:source       mimdoc:sectie2.8.2.22 ;
        skos:definition  "Als het type van het attribuutsoort een waardenlijst is, dan wordt hier de locatie waar deze te vinden is opgegeven."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "locatie"@nl .

mim:ConceptueelInformatiemodel
        a               skos:Concept ;
        rdfs:label      "Conceptueel informatiemodel" ;
        skos:inScheme   <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel  "Conceptueel" .

mimbegrip:specificatieFormeel
        a                skos:Concept ;
        rdfs:label       "specificatie formeel"@nl ;
        skos:definition  "De beschrijving van de constraint in een formele specificatietaal, in OCL"@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "specificatie formeel"@nl .

mimbegrip:informatiedomein
        a                skos:Concept ;
        rdfs:label       "informatiedomein"@nl ;
        dct:source       mimdoc:sectie2.8.1.1 ;
        skos:definition  "Aanduiding van het functionele domein waartoe het informatiemodel behoort."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "informatiedomein"@nl .

mimbegrip:ExterneKoppeling
        a                skos:Concept ;
        rdfs:label       "Externe koppeling"@nl ;
        dct:source       mimdoc:sectie2.3.4 ;
        skos:broader     mimbegrip:Modelelement ;
        skos:definition  "Een associatie waarmee vanuit het perspectief van het eigen informatiemodel een objecttype uit het ‘eigen’ informatiemodel gekoppeld wordt aan een objecttype van een extern informatiemodel. De relatie zelf hoort bij het ‘eigen’ objecttype."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "Externe koppeling"@nl .

mim:Basisgegeven  a      skos:Concept ;
        rdfs:label       "Basisgegeven" ;
        skos:definition  "Een in een basisregistratie opgenomen gegeven."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "Basisgegeven" .

mim:LandelijkKerngegeven
        a                skos:Concept ;
        rdfs:label       "LandelijkKerngegeven" ;
        skos:definition  "Indien het een gegeven of een als relatiesoort gemodelleerd gegeven is in een landelijk sector- en domein-overstijgend informatiemodel en geen authentiek gegeven en geen basisgegeven is."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "Landelijk kerngegeven" .

mimbegrip:waarde  a      skos:Concept ;
        rdfs:label       "waarde"@nl ;
        dct:source       mimdoc:sectie2.8.4.9 ;
        skos:definition  "De binding van een enumeratiewaarde aan een enumeratie."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "waarde"@nl .

mimbegrip:herkomstDefinitie
        a                skos:Concept ;
        rdfs:label       "herkomst definitie"@nl ;
        dct:source       mimdoc:sectie2.8.2.6 ;
        skos:definition  "De registratie of het informatiemodel waaruit de definitie is overgenomen dan wel een aanduiding die aangeeft uit welke bronnen de definitie is samengesteld."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "herkomst definitie"@nl .

mimbegrip:Generalisatie
        a                skos:Concept ;
        rdfs:label       "Generalisatie"@nl ;
        dct:source       mimdoc:sectie2.3.1 ;
        skos:broader     mimbegrip:Modelelement ;
        skos:definition  "De typering van het hiërarchische verband tussen een meer generiek en een meer specifiek modelelement van hetzelfde soort, waarbij het meer specifieke modelelement eigenschappen van het meer generieke modelelement overerft. Dit verband is alleen gedefinieerd voor objecttypen en datatypen."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "Generalisatie"@nl .

mimbegrip:indicatieFormeleHistorie
        a                skos:Concept ;
        rdfs:label       "Indicatie formele historie"@nl ;
        dct:source       mimdoc:sectie2.8.2.11 ;
        skos:definition  "Indicatie of de formele historie van het kenmerk van het object bijgehouden wordt en te bevragen is."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "Indicatie formele historie"@nl .

mimbegrip:Referentielijst
        a                skos:Concept ;
        rdfs:label       "Referentielijst"@nl ;
        dct:source       mimdoc:sectie2.4.1 ;
        skos:broader     mimbegrip:Datatype ;
        skos:definition  "De representatie van een lijst met een opsomming van de mogelijke domeinwaarden van een attribuutsoort, die buiten het model in een externe waardenlijst worden beheerd. De domeinwaarden in de lijst kunnen in de loop van de tijd aangepast, uitgebreid, of verwijderd worden, zonder dat het informatiemodel aangepast wordt (in tegenstelling tot bij een enumeratie). De representatie bevat een aantal kenmerken, die overgenomen zijn van de specificatie van de externe waardelijst."@nl ;
        skos:inScheme    <http://definities.mim-standaard.nl/id/begrippenkader/MIM> ;
        skos:prefLabel   "Referentielijst"@nl .

mimbegrip:lengte  a      skos:Concept ;
        rdfs:label       "lengte"@nl ;
        dct:source       mimdoc:sectie2.8.2.24 ;
