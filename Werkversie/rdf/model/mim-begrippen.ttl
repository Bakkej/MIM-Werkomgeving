<http://bp4mc2.org/mim/id/begrip/Package>
        a                skos:Concept ;
        rdfs:label       "Package"@nl ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Modelelement> ;
        skos:definition  "Een package is een benoemde en begrensde verzameling/groepering van modelelementen."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

mim:KeuzeConstraint  dct:subject  <http://bp4mc2.org/mim/id/begrip/KeuzeConstraint> .

<http://bp4mc2.org/mim/id/begrip/Constraint>
        a                skos:Concept ;
        rdfs:label       "Constraint"@nl ;
        dct:source       mimdoc:sectie2.10.1 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Modelelement> ;
        skos:definition  "Een constraint is een conditie of een beperking, die over een of meerdere modelelementen uit het informatiemodel geldt."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

<http://bp4mc2.org/mim/id/begrip/KeuzeConstraint>
        a                skos:Concept ;
        rdfs:label       "Keuze"@nl ;
        dct:source       mimdoc:sectie2.10.2 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Constraint> ;
        skos:definition  "Een conditie met een opsomming van meerdere modelelementen, waarbij er maar van één tegelijkertijd sprake kan zijn."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

mim:RelatierolDoel  dct:subject  <http://bp4mc2.org/mim/id/begrip/RelatierolDoel> .

<http://bp4mc2.org/mim/id/begrip/View>
        a                skos:Concept ;
        rdfs:label       "View"@nl ;
        dct:source       mimdoc:sectie2.9.4 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Package> ;
        skos:definition  "Een groepering van objecttypen die gespecificeerd zijn in een extern informatiemodel en vanuit het perspectief van het eigen informatiemodel inzicht geeft welke gegevens van deze objecttypen relevant zijn binnen het eigen informatiemodel."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

mim:Gegevensgroep  dct:subject  <http://bp4mc2.org/mim/id/begrip/Gegevensgroep> .

<http://bp4mc2.org/mim/id/begrip/Gegevensgroeptype>
        a                skos:Concept ;
        rdfs:label       "Gegevensgroeptype"@nl ;
        dct:source       mimdoc:sectie2.5.5 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Modelelement> ;
        skos:definition  "Een groep van met elkaar samenhangende attribuutsoorten. Een gegevensgroeptype is altijd een type van een gegevensgroep."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

<http://bp4mc2.org/mim/id/begrip/PrimitiefDatatype>
        a                skos:Concept ;
        rdfs:label       "Primitief datatype"@nl ;
        dct:source       mimdoc:sectie2.8.1 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Datatype> ;
        skos:definition  "Een in het eigen model gedefinieerd datatype die gebaseerd is op een PrimitiveType, met een eigen naam en definitie."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

<http://bp4mc2.org/mim/id/begrip/Keuze>
        a                skos:Concept ;
        rdfs:label       "Keuze"@nl ;
        dct:source       mimdoc:sectie2.10.2 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Datatype> ;
        skos:definition  "Een opsomming van meerdere modelelementen, waarbij er maar van één tegelijkertijd sprake kan zijn."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

mim:PrimitiefDatatype
        dct:subject  <http://bp4mc2.org/mim/id/begrip/PrimitiefDatatype> .

