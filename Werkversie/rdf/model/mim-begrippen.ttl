        dct:source       mimdoc:sectie2.6.5 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Modelelement> ;
        skos:definition  "De benaming van de manier waarop een object deelneemt aan een relatie met een ander object."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

