mim:Referentielijst  dct:subject  <http://bp4mc2.org/mim/id/begrip/Referentielijst> .

<http://bp4mc2.org/mim/id/begrip/Gegevensgroep>
        a                skos:Concept ;
        rdfs:label       "Gegevensgroep"@nl ;
        dct:source       mimdoc:sectie2.5.4 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Modelelement> ;
        skos:definition  "Een typering van een groep van gelijksoortige gegevens die voor een objecttype van toepassing is."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

mim:Modelelement  dct:subject  <http://bp4mc2.org/mim/id/begrip/Modelelement> .

mim:Codelijst  dct:subject  <http://bp4mc2.org/mim/id/begrip/Codelijst> .

mim:RelatierolBron  dct:subject  <http://bp4mc2.org/mim/id/begrip/RelatierolBron> .

<http://bp4mc2.org/mim/id/begrip/Codelijst>
        a                skos:Concept ;
        rdfs:label       "Codelijst"@nl ;
        dct:source       mimdoc:sectie2.7.5 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Datatype> ;
        skos:definition  "De representatie van een lijst met een opsomming van de mogelijke domeinwaarden van een attribuutsoort, die buiten het model in een externe waardenlijst worden beheerd. De domeinwaarden in de lijst kunnen in de loop van de tijd aangepast, uitgebreid, of verwijderd worden, zonder dat het informatiemodel aangepast wordt (in tegenstelling tot bij een enumeratie). De representatie bevat geen kenmerken, voor alle kenmerken wordt verwezen naar de specificatie van de externe waardelijst."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

mim:DataElement  dct:subject  <http://bp4mc2.org/mim/id/begrip/DataElement> .

mim:Extern  dct:subject  <http://bp4mc2.org/mim/id/begrip/Extern> .

<http://bp4mc2.org/mim/id/begrip/Domein>
        a                skos:Concept ;
        rdfs:label       "Domein"@nl ;
        dct:source       mimdoc:sectie2.9.2 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Package> ;
        skos:definition  "Een groepering van constructies die een semantisch samenhangend gedeelte van een informatiemodel beschrijven."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

<http://bp4mc2.org/mim/id/begrip/GestructureerdDatatype>
        a                skos:Concept ;
        rdfs:label       "Gestructureerd datatype"@nl ;
        dct:source       mimdoc:sectie2.8.2 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Datatype> ;
        skos:definition  "Specifiek benoemd datatype dat de structuur van een gegeven beschrijft, samengesteld uit minimaal twee elementen die in samenhang betekenisvol zijn."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

mim:Datatype  dct:subject  <http://bp4mc2.org/mim/id/begrip/Datatype> .

<http://bp4mc2.org/mim/id/begrip/ExterneKoppeling>
        a                skos:Concept ;
        rdfs:label       "Externe koppeling"@nl ;
        dct:source       mimdoc:sectie2.6.4 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Modelelement> ;
        skos:definition  "Een associatie waarmee vanuit het perspectief van het eigen informatiemodel een objecttype uit het ‘eigen’ informatiemodel gekoppeld wordt aan een objecttype van een extern informatiemodel. De relatie zelf hoort bij het ‘eigen’ objecttype."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

<http://bp4mc2.org/mim/id/begrip/Generalisatie>
        a                skos:Concept ;
        rdfs:label       "Generalisatie"@nl ;
        dct:source       mimdoc:sectie2.6.1 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Modelelement> ;
        skos:definition  "De typering van het hiërarchische verband tussen een meer generiek en een meer specifiek modelelement van hetzelfde soort, waarbij het meer specifieke modelelement eigenschappen van het meer generieke modelelement overerft. Dit verband is alleen gedefinieerd voor objecttypen en datatypen."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

mim:GestructureerdDatatype
        dct:subject  <http://bp4mc2.org/mim/id/begrip/GestructureerdDatatype> .

<http://bp4mc2.org/mim/id/begrip/Enumeratiewaarde>
        a                skos:Concept ;
        rdfs:label       "Enumeratiewaarde"@nl ;
        dct:source       mimdoc:sectie2.7.4 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Modelelement> ;
        skos:definition  "Een gedefinieerde waarde, in de vorm van een eenmalig vastgesteld constant gegeven."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

<http://bp4mc2.org/mim/id/begrip/RelatierolDoel>
        a                skos:Concept ;
        rdfs:label       "Relatierol doel"@nl ;
        dct:source       mimdoc:sectie2.6.5 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Relatierol> ;
        skos:definition  "De relatierol die de rol beschrijft van het doel van de relatie." ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

<http://bp4mc2.org/mim/id/begrip/ReferentieElement>
        a                skos:Concept ;
        rdfs:label       "Referentie element"@nl ;
        dct:source       mimdoc:sectie2.7.2 ;
        skos:broader     <http://bp4mc2.org/mim/id/begrip/Modelelement> ;
        skos:definition  "Een eigenschap van een object in een referentielijst in de vorm van een gegeven."@nl ;
        skos:inScheme    <http://bp4mc2.org/id/begrippenkader/mim> .

mim:ReferentieElement
        dct:subject  <http://bp4mc2.org/mim/id/begrip/ReferentieElement> .
